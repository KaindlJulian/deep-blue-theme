{
    // Theme Color reference.
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference
    "$schema": "vscode://schemas/color-theme",
    "name": "Midnight Blue",
    "type": "dark",
    "colors": {
        // Defaults
        "focusBorder": "#0000",
        "foreground": "#7B82DF",

        // Activity Bar.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_activity-bar
        "activityBar.background": "#05081f",
        "activityBar.border": "#070929",
        "activityBar.dropBackground": "#7B82DF40",
        "activityBar.foreground": "#F2F2F2",
        "activityBar.inactiveForeground": "#7B82DF",
        "activityBarBadge.foreground": "#0A0E41",
        "activityBarBadge.background": "#F7E03C",

        // Sidebar.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_side-bar
        "sideBar.background": "#070b2b",
        "sideBar.border": "#070b2b",
        "sideBar.foreground": "#7B82DF",
        "sideBarSectionHeader.background": "#0f1750",
        "sideBarSectionHeader.foreground": "#9ca3ff",
        "sideBarTitle.foreground": "#f2f2f2",
        "sideBarSectionHeader.border": "#0f1750",

        // Badge.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_badge
        "badge.background": "#F7E03C",
        "badge.foreground": "#0A0E41",

        // Button.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_button-control
        "button.background": "#f7e13cb2",
        "button.foreground": "#0A0E41",
        "button.hoverBackground": "#F7E03C",

        // Contrast. (are transparent because this is not an high conrast theme)
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_contrast-colors
        "contrastActiveBorder": "#fff0",
        "contrastBorder": "#fff0",

        // Base Colors.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_base-colors
        // Foreground color for description text providing additional information, for example for a label.
        "descriptionForeground": "#7B82DF",
        "selection.background": "#4f6fff7e",

        // Dropdown.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_dropdown-control
        "dropdown.background": "#090e36",
        "dropdown.border": "#090e36",
        "dropdown.foreground": "#F2F2F2",

        // ——— Editor ———
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_editor-colors
        "editor.background": "#090e36", // Editor background color.
        "editor.foreground": "#F2F2F2", // Editor default foreground color.
        "editorLineNumber.foreground": "#7B82DF", // Color of editor line numbers.
        "editorCursor.foreground": "#F7E03C", //  Color of the editor cursor.

        //  The selection color
        "editor.selectionBackground": "#4f6fff7e", // Color of the editor selection.
        "editor.inactiveSelectionBackground": "#4b56868f", // Color of the selection in an inactive editor. The color must not be opaque to not hide underlying decorations.
        "editor.selectionHighlightBackground": "#58588173", // Color for regions with the same content as the selection. The color must not be opaque to not hide underlying decorations.
        "editor.wordHighlightBackground": "#F2F2F20D", // Same words other places.
        "editor.wordHighlightStrongBackground": "#F2F2F20D", // Cursor inside current variable.
        "editor.findMatchBackground": "#ff7300ab", // Color of the current search match.
        "editor.findMatchHighlightBackground": "#FFFF0336", // Color of the other search matches.The color must not be opaque to not hide underlying decorations.
        "editor.findRangeHighlightBackground": "#FFFF0336", // #FF730056 No idea. Color the range limiting the search (Enable 'Find in Selection' in the find widget). The color must not be opaque to not hide underlying decorations.
        "editor.hoverHighlightBackground": "#3c5ef77e", // Highlight below the word for which a hover is shown. The color must not be opaque to not hide underlying decorations
        "editor.lineHighlightBackground": "#05081f", // Current line of code. Background color for the highlight of line at the cursor position.
        "editor.lineHighlightBorder": "#05081f",
        "editor.rangeHighlightBackground": "#2f365e",
        "editorLink.activeForeground": "#F7E03C",
        "editorIndentGuide.background": "#070b2b",
        "editorIndentGuide.activeBackground": "#2f365e",
        "editorRuler.foreground": "#070b2b", // Editor Ruler.
        "editorOverviewRuler.border": "#070b2b",
        "editorCodeLens.foreground": "#7B82DF",
        "editorBracketMatch.background": "#4b56868f",
        "editorBracketMatch.border": "#4b56868f",

        // Overview ruler is located beneath the scroll bar on the right edge of the editor and gives an overview of the decorations in the editor.
        "editorOverviewRuler.commonContentForeground": "#FFC60055",
        "editorOverviewRuler.currentContentForeground": "#EE3A4355",
        "editorOverviewRuler.incomingContentForeground": "#3AD90055",

        // Errors and warnings:
        // "editorError.border": "#EC3A37F5",
        "editorError.foreground": "#FF0000",
        "editorWarning.border": "#fff0",
        "editorWarning.foreground": "#ffae00",

        // Gutter: The gutter contains the glyph margins and the line numbers:
        "editorGutter.background": "#090e36",
        "editorGutter.addedBackground": "#35AD68",
        "editorGutter.deletedBackground": "#f71d1dd5",
        "editorGutter.modifiedBackground": "#218DA580",

        // Diff Editor.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_diff-editor-colors
        "diffEditor.insertedTextBackground": "#00FF000E",
        "diffEditor.insertedTextBorder": "#00FF009A",
        "diffEditor.removedTextBackground": "#ff000018",
        "diffEditor.removedTextBorder": "#ff000056",

        // Editor Groups & Tabs.
        "editorGroup.border": "#05081f",
        "editorGroup.dropBackground": "#05081f80",

        // The editorGroupHeader.
        "editorGroupHeader.noTabsBackground": "#05081f",
        "editorGroupHeader.tabsBackground": "#05081f",
        "editorGroupHeader.tabsBorder": "#05081f",

        // The tabs
        "tab.activeBackground": "#2f365e",
        "tab.activeForeground": "#F2F2F2",
        "tab.border": "#090e36",
        "tab.activeBorder": "#F7E03C",
        "tab.inactiveBackground": "#070b2b",
        "tab.inactiveForeground": "#7B82DF",
        "tab.unfocusedActiveForeground": "#070b2b",
        "tab.unfocusedInactiveForeground": "#7B82DF",

        // The Editor widget is shown in front of the editor content. Examples are the Find/Replace dialog, the suggestion widget, and the editor hover.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_editor-widget-colors
        "editorWidget.background": "#2f365e",
        "editorWidget.border": "#2f365e",
        "editorHoverWidget.background": "#070b2b",
        "editorHoverWidget.border": "#1d2774",
        "editorSuggestWidget.background": "#070b2b",
        "editorSuggestWidget.border": "#1d2774",
        "editorSuggestWidget.foreground": "#7B82DF",
        "editorSuggestWidget.highlightForeground": "#F7E03C",
        "editorSuggestWidget.selectedBackground": "#2f365e",

        // Debug.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_debug
        "debugToolBar.background": "#2f365e",
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_editor-widget-colors
        "debugExceptionWidget.background": "#2f365e",
        "debugExceptionWidget.border": "#2f365e",

        // The editor marker view shows when navigating to errors and warnings in the editor (Go to Next Error or Warning command).
        "editorMarkerNavigation.background": "#3B536433",
        "editorMarkerNavigationError.background": "#EC3A37F5",
        "editorMarkerNavigationWarning.background": "#F7E03C",

        // To see the editor white spaces, enable Toggle Render Whitespace.
        "editorWhitespace.foreground": "#F2F2F21A",
        "errorForeground": "#EC3A37F5",

        // Extensions.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_extensions
        "extensionButton.prominentBackground": "#f7e13cb2",
        "extensionButton.prominentForeground": "#0A0E41",
        "extensionButton.prominentHoverBackground": "#F7E03C",

        // Input Control.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_input-control
        "input.background": "#070929",
        "input.border": "#0f1750",
        "input.foreground": "#ffec60",
        "input.placeholderForeground": "#7B82DF",
        "inputOption.activeBorder": "#fff0",
        "inputValidation.errorBackground": "#070929",
        "inputValidation.errorBorder": "#EC3A37F5", // search for "ssd\"
        "inputValidation.infoBackground": "#070929",
        "inputValidation.infoBorder": "#070929",
        "inputValidation.warningBackground": "#070929",
        "inputValidation.warningBorder": "#ffec60",
        "inputOption.activeBackground": "#fff0",

        // Lists and Trees.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_lists-and-trees
        "list.dropBackground": "#2f365e80",
        "list.activeSelectionBackground": "#2f365e",
        "list.activeSelectionForeground": "#F2F2F2",
        "list.focusBackground": "#070929",
        "list.focusForeground": "#F2F2F2",
        "list.highlightForeground": "#F2F2F2",
        "list.hoverBackground": "#2f365e80",
        "list.hoverForeground": "#CEC5FF",
        "list.inactiveSelectionBackground": "#2f365e80",
        "list.inactiveSelectionForeground": "#f2f2f2",
        "tree.indentGuidesStroke": "#7B82DF",
        "list.warningForeground": "#ffec60",
        "list.errorForeground": "#ec3a37",

        // Tree List Filter Colors.
        "listFilterWidget.background": "#2f365e",
        "listFilterWidget.outline": "#2f365e",
        "listFilterWidget.noMatchesOutline": "#EC3A37F5",

        // Merge Conflicts.
        "merge.border": "#F2F2F200",
        "merge.commonContentBackground": "#7B82DF",
        "merge.commonHeaderBackground": "#7B82DF",
        "merge.currentContentBackground": "#3ad90093",
        "merge.currentHeaderBackground": "#3ad90093",
        "merge.incomingContentBackground": "#ff730086",
        "merge.incomingHeaderBackground": "#ff730086",

        // Notification Colors.
        "notificationCenter.border": "#0f1750", // Notification Center border color.
        "notificationCenterHeader.foreground": "#F2F2F2", // Notification Center header foreground color.
        "notificationCenterHeader.background": "#0f1750", // Notification Center header background color.
        "notificationToast.border": "#2f365e", // Notification toast border color.
        "notifications.foreground": "#f2f2f2", // Notification foreground color.
        "notifications.background": "#2f365e", // Notification background color.
        "notifications.border": "#070929", // Notification border color separating from other notifications in the Notification Center.
        "notificationLink.foreground": "#F2F2F2", // Notification links foreground color.

        // Panel.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_panel-colors
        "panel.background": "#070b2b",
        "panel.border": "#070b2b",
        "panelTitle.activeBorder": "#F7E03C",
        "panelTitle.activeForeground": "#F7E03C",
        "panelTitle.inactiveForeground": "#7B82DF",

        // Peek View Colors.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_peek-view-colors
        "peekView.border": "#f7e13cb2",
        "peekViewEditor.background": "#070b2b",
        "peekViewEditor.matchHighlightBackground": "#3c5ef77e",
        "peekViewEditorGutter.background": "#090e36",
        "peekViewResult.background": "#05081f",
        "peekViewResult.fileForeground": "#dddddd",
        "peekViewResult.lineForeground": "#F2F2F2",
        "peekViewResult.matchHighlightBackground": "#070929",
        "peekViewResult.selectionBackground": "#070929",
        "peekViewResult.selectionForeground": "#F2F2F2",
        "peekViewTitle.background": "#05081f",
        "peekViewTitleDescription.foreground": "#dddddd",
        "peekViewTitleLabel.foreground": "#F7E03C",

        // Quick Picker.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_quick-picker
        "pickerGroup.border": "#7B82DF",
        "pickerGroup.foreground": "#F7E03C",

        // Progress Bar.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_progress-bar
        "progressBar.background": "#F7E03C",

        // Scroll Bar Control.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_scroll-bar-control
        "scrollbar.shadow": "#000002c9",
        "scrollbarSlider.background": "#22295f80",
        "scrollbarSlider.hoverBackground": "#0f1750",
        "scrollbarSlider.activeBackground": "#f7e13ca8",

        // Status Bar Colors.
        // The Status Bar is shown in the bottom of the workbench.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_status-bar-colors
        "statusBar.border": "#05081f",
        "statusBar.background": "#05081f",
        "statusBar.foreground": "#7B82DF",
        "statusBar.debuggingBackground": "#F7E03C",
        "statusBar.debuggingForeground": "#0A0E41",
        "statusBar.noFolderBackground": "#F7E03C",
        "statusBar.noFolderForeground": "#0A0E41",
        "statusBarItem.activeBackground": "#7B82DF59",
        "statusBarItem.hoverBackground": "#7B82DF40",
        "statusBarItem.prominentBackground": "#f7e13ca8",
        "statusBarItem.prominentHoverBackground": "#F7E03C",

        // Integrated Terminal Colors.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_integrated-terminal-colors
        "terminal.ansiBlack": "#000000",
        "terminal.ansiRed": "#EC3A37F5",
        "terminal.ansiGreen": "#3AD900",
        "terminal.ansiYellow": "#F7E03C",
        "terminal.ansiBlue": "#6943FF",
        "terminal.ansiMagenta": "#FF2C70",
        "terminal.ansiCyan": "#80FCFF",
        "terminal.ansiWhite": "#F2F2F2",
        "terminal.ansiBrightBlack": "#555753",
        "terminal.ansiBrightRed": "#EF2929",
        "terminal.ansiBrightGreen": "#8AE234",
        "terminal.ansiBrightYellow": "#FCE94F",
        "terminal.ansiBrightBlue": "#729FCF",
        "terminal.ansiBrightMagenta": "#AD7FA8",
        "terminal.ansiBrightCyan": "#9EFFFF",
        "terminal.ansiBrightWhite": "#F2F2F2",
        "terminal.background": "#070b2b",
        "terminal.foreground": "#F2F2F2",
        "terminalCursor.background": "#f2f2f2",
        "terminalCursor.foreground": "#f2f2f2",
        "terminal.selectionBackground": "#4f6fff7e",

        // Git VS Code theme Colors used for file labels and the SCM viewlet.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_git-colors
        "gitDecoration.modifiedResourceForeground": "#ffec60", // Color for modified git resources.
        "gitDecoration.deletedResourceForeground": "#EC3A37F5", // Color for deleted git resources.
        "gitDecoration.untrackedResourceForeground": "#3AD900", // Color for untracked git resources.
        "gitDecoration.ignoredResourceForeground": "#99a1e981", // Color for ignored git resources.
        "gitDecoration.conflictingResourceForeground": "#FF7200", // Color for conflicting git resources.

        // Text Colors — Colors inside a text document, such as the welcome page.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_text-colors
        "textBlockQuote.background": "#1E1E3F",
        "textBlockQuote.border": "#6943FF",
        "textCodeBlock.background": "#1E1E3F",
        "textLink.activeForeground": "#F7E03C",
        "textLink.foreground": "#ddd",
        //"textPreformat.foreground": "#ff0000",
        //"textSeparator.foreground": "#ff0000",

        // Title bar.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_title-bar-colors
        "titleBar.activeBackground": "#05081f",
        "titleBar.activeForeground": "#F2F2F2",
        "titleBar.inactiveBackground": "#070b2b",
        "titleBar.inactiveForeground": "#ddd",
        "walkThrough.embeddedEditorBackground": "#05081f",
        "welcomePage.buttonBackground": "#2f365e",
        "welcomePage.buttonHoverBackground": "#2f365e94",
        "widget.shadow": "#00000050",

        // Breadcrumbs.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_breadcrumbs
        "breadcrumb.foreground": "#7B82DF",
        "breadcrumb.focusForeground": "#F7E03C", // Color of focused breadcrumb items.
        "breadcrumb.activeSelectionForeground": "#F2F2F2", // Color of selected breadcrumb items.
        "breadcrumbPicker.background": "#070b2b", // Background color of breadcrumb item picker.

        // New Settings Editor.
        // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_settings-editor-colors
        "settings.headerForeground": "#F2F2F2",
        "settings.dropdownBackground": "#070929",
        "settings.checkboxBackground": "#070929",
        "settings.textInputBackground": "#070929",
        "settings.numberInputBackground": "#070929",
        "settings.dropdownForeground": "#E5E4FB",
        "settings.checkboxForeground": "#E5E4FB",
        "settings.textInputForeground": "#E5E4FB",
        "settings.numberInputForeground": "#E5E4FB",
        "settings.dropdownBorder": "#0f1750",
        "settings.checkboxBorder": "#0f1750",
        "settings.textInputBorder": "#0f1750",
        "settings.numberInputBorder": "#0f1750",
        "settings.dropdownListBorder": "#070929",
        "settings.modifiedItemIndicator": "#F7E03C",

        // Customizable menu.
        "menu.separatorBackground": "#7B82DF",

        // Tab stops.
        "editor.snippetTabstopHighlightBackground": "#4f6fff7e",
        "editor.snippetTabstopHighlightBorder": "#4f6fff7e",
        "editor.snippetFinalTabstopHighlightBackground": "#58588173",
        "editor.snippetFinalTabstopHighlightBorder": "#58588173"

        // TODO: Colors to be added.
        // "inputValidation.infoForeground": "#F7E03C", // Input validation foreground color for information severity.
        // "inputValidation.warningForeground": "#00000000", // Input validation foreground color for warning severity.
        // "inputValidation.errorForeground": "#EC3A37F5", // Input validation foreground color for error severity.
    },
    // Token Colors are heavily inspired by several themes
    // Including but not limited to Material Palenight, Cobalt
    // theme's syntax and several custom setup via Dev scope ext.
    "tokenColors": [
        {
            "name": "[COMMENTS] — The main comments color",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#5d849e"
            }
        },
        {
            "name": "[Entity] — The main Entity color",
            "scope": "entity",
            "settings": {
                "foreground": "#F7E03C"
            }
        },
        {
            "name": "[Constant] — The main constants color",
            "scope": "constant",
            "settings": {
                "foreground": "#FF628C"
            }
        },
        {
            "name": "[Keyword] — The main color for Keyword",
            "scope": ["keyword, storage.type.class.js"],
            "settings": {
                "foreground": "#FF9D00"
            }
        },
        {
            "name": "[Meta] — The main color for Meta",
            "scope": "meta",
            "settings": {
                "foreground": "#9EFFFF"
            }
        },
        {
            "name": "[invalid] — The main color for invalid",
            "scope": "invalid",
            "settings": {
                "foreground": "#EC3A37F5"
            }
        },
        {
            "name": "[Meta Brace] — The main color for Meta Brace",
            "scope": ["meta.brace", "punctuation.definition.parameters.begin.js", "punctuation.definition.parameters.end.js"],
            "settings": {
                "foreground": "#E1EFFF"
            }
        },
        {
            "name": "[Punctuation] — Color for end of line punctuation",
            "scope": "punctuation.terminator",
            "settings": {
                "foreground": "#E1EFFF"
            }
        },
        {
            "name": "[Punctuation] — Color for Punctuation Parameters",
            "scope": "punctuation.definition.parameters",
            "settings": {
                "foreground": "#fff3a3"
            }
        },
        {
            "name": "[Punctuation] — Color for Punctuation Template Expression",
            "scope": "punctuation.definition.template-expression",
            "settings": {
                "foreground": "#fff3a3"
            }
        },
        {
            "name": "[Storage] — The main color for Storage",
            "scope": "storage",
            "settings": {
                "foreground": "#F7E03C"
            }
        },
        {
            "name": "[Storage] — The color for Storage Type Arrow Function",
            "scope": "storage.type.function.arrow",
            "settings": {
                "foreground": "#F7E03C"
            }
        },
        {
            "name": "[String]",
            "scope": ["string", "punctuation.definition.string"],
            "settings": {
                "foreground": "#89ff6e"
            }
        },
        {
            "name": "[String] Template Color",
            "scope": ["string.template", "punctuation.definition.string.template"],
            "settings": {
                "foreground": "#3AD900"
            }
        },
        {
            "name": "[Support]",
            "scope": "support",
            "settings": {
                "foreground": "#80FFBB"
            }
        },
        {
            "name": "[Support] Function Colors",
            "scope": "support.function",
            "settings": {
                "foreground": "#FF9D00"
            }
        },
        {
            "name": "[Support] Variable Property DOM Colors",
            "scope": "support.variable.property.dom",
            "settings": {
                "foreground": "#9EFFFF"
            }
        },
        {
            "name": "[Variable]",
            "scope": "variable",
            "settings": {
                "foreground": "#E1EFFF"
            }
        },
        {
            "name": "[INI] - Color for Entity",
            "scope": ["source.ini entity", "meta.embedded.block.ini", "source.ini"],
            "settings": {
                "foreground": "#E1EFFF"
            }
        },
        {
            "name": "[INI] - Color for Keyword",
            "scope": ["source.ini keyword", "keyword.other.definition.ini"],
            "settings": {
                "foreground": "#F7E03C"
            }
        },
        {
            "name": "[INI] - Color for Punctuation Definition",
            "scope": ["source.ini punctuation.definition"],
            "settings": {
                "foreground": "#fff3a3"
            }
        },
        {
            "name": "[INI] - Color for Punctuation Separator",
            "scope": ["source.ini punctuation.separator", "punctuation.separator.key-value.ini"],
            "settings": {
                "foreground": "#FF9D00"
            }
        },
        {
            "name": "[CSS] - Color for Entity",
            "scope": ["source.css entity", "source.stylus entity"],
            "settings": {
                "foreground": "#3AD900"
            }
        },
        {
            "name": "[CSS] - Color for Class Selector",
            "scope": "entity.other.attribute-name.class.css",
            "settings": {
                "foreground": "#F7E03C"
            }
        },
        {
            "name": "[CSS] - Color for ID Selector",
            "scope": ["entity.other.attribute-name.id.css"],
            "settings": {
                "foreground": "#FF9D00"
            }
        },
        {
            "name": "[CSS] - Color for Element Selector",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#9EFFFF"
            }
        },
        {
            "name": "[CSS] - Color for Support",
            "scope": ["source.css support", "entity.name.tag.css", "source.stylus support"],
            "settings": {
                "foreground": "#9EFFFF"
            }
        },
        {
            "name": "[CSS] - Color for Constant Property Values",
            "scope": ["support.constant.property-value.css"],
            "settings": {
                "foreground": "#ff5a57"
            }
        },
        {
            "name": "[CSS] - Color for Constant",
            "scope": [
                "source.css constant",
                "source.css support.constant",
                "source.stylus constant",
                "source.stylus support.constant"
            ],
            "settings": {
                "foreground": "#FB94FF"
            }
        },
        {
            "name": "[CSS] - Color for Units",
            "scope": ["keyword.other.unit"],
            "settings": {
                "foreground": "#F7E03C"
            }
        },
        {
            "name": "[CSS] - Color for String and Url Parameters",
            "scope": [
                "source.css string",
                "source.css punctuation.definition.string",
                "source.stylus string",
                "source.stylus punctuation.definition.string",
                "variable.parameter.url.scss"
            ],
            "settings": {
                "foreground": "#89ff6e"
            }
        },
        {
            "name": "[CSS] - Color for Variable",
            "scope": ["source.css variable", "source.stylus variable"],
            "settings": {
                "foreground": "#FF9D00"
            }
        },
        {
            "name": "[SCSS] - Color for Ampersand",
            "scope": ["entity.name.tag.reference.scss"],
            "settings": {
                "foreground": "#F7E03C"
            }
